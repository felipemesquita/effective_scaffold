<h1>Listing <%= plural_name %></h1>

<ol class="<%= plural_name %>_list">
  <li class="<%= singular_name %>">
  <% for attribute in attributes -%>
    <div class="<%= singular_name %>_<%= attribute.name %>"><%%=h attribute.<%= attribute.name %> %></div>
  <% end -%>
    <div class="admin_<%= singular_name %>">
      <div><%%= link_to 'Show', <%= singular_name %>_path %></div>
      <div><%%= link_to 'Edit', edit_<%= singular_name %>_path(@<%= singular_name %>) %></div>
      <div><%%= link_to 'Destroy', <%= singular_name %>_path(@<%= singular_name %>), :confirm => 'Are you sure?', :method => :delete %></div>
    </div>
  </li>
</ol>

<%%= link_to 'New <%= singular_name %>', new_<%= singular_name %>_path %>
